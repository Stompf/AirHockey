{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "wwwroot": true,
        "bin": true,
        "obj": true,
        ".gitattributes": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.jsx": {
            "when": "$(basename).tsx"
        },
        "**/*.js.map": true,
        "publish": true,
        "*.log": true,
        "npm-debug.log*": true,
        ".vscode/chrome": true
    },
    "vsicons.presets.angular": true,
    "debug.toolBarLocation": "docked",
    "terminal.integrated.copyOnSelection": true,
    "typescript.tsdk": "./node_modules/typescript/lib",
    "emmet.showExpandedAbbreviation": "never",
    "editor.codeActionsOnSave": {
        "source.fixAll.tslint": true
    },
    "cSpell.words": [
        "RESPAWN",
        "Unmount",
        "renderable"
    ]
}
