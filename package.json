{
    "name": "lunne-web",
    "version": "1.0.0",
    "description": "Lunne Web",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start-server\" \"npm run start-client-dev\"",
        "start-server": "nodemon",
        "server-watch": "tsc -p ./src/server/tsconfig.json --watch",
        "build-server-prod": "tsc -p ./src/server/tsconfig.json",
        "start-client-dev": "webpack --config=./webpack.dev.js --mode development --watch",
        "build-client-prod": "webpack --config=./webpack.prod.js --mode production",
        "build-all-prod": "npm run build-client-prod && npm run build-server-prod",
        "start-prod": "NODE_ENV=production node server-dist/server/index.js",
        "build-docker-prod": "docker build -t stompf/lunne-web  .",
        "push-to-docker": "docker login && docker push stompf/lunne-web:latest",
        "build-and-push": "npm run build-docker-prod && npm run push-to-docker",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Stompf/AirHockey.git"
    },
    "author": "Anton Lundmark",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Stompf/AirHockey/issues"
    },
    "homepage": "https://github.com/Stompf/AirHockey#readme",
    "dependencies": {
        "@material-ui/core": "4.11.2",
        "@material-ui/icons": "4.11.2",
        "event-emitter": "0.3.5",
        "express": "4.17.1",
        "helmet": "4.2.0",
        "matter-js": "0.14.2",
        "module-alias": "2.2.2",
        "moment": "2.29.1",
        "morgan": "1.10.0",
        "p2": "0.7.1",
        "phaser": "3.24.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-router": "5.2.0",
        "react-router-dom": "5.2.0",
        "socket.io": "3.0.4",
        "socket.io-client": "3.0.4",
        "winston": "3.3.3"
    },
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@types/event-emitter": "0.3.3",
        "@types/express": "4.17.9",
        "@types/helmet": "0.0.48",
        "@types/matter-js": "0.14.8",
        "@types/module-alias": "2.0.0",
        "@types/morgan": "1.9.2",
        "@types/node": "14.14.14",
        "@types/p2": "0.7.36",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/react-router": "5.1.8",
        "@types/react-router-dom": "5.1.6",
        "@types/socket.io": "2.1.12",
        "@types/socket.io-client": "1.4.34",
        "@typescript-eslint/eslint-plugin": "4.10.0",
        "@typescript-eslint/parser": "4.10.0",
        "babel-loader": "8.2.2",
        "babel-preset-env": "1.7.0",
        "concurrently": "5.3.0",
        "copy-webpack-plugin": "7.0.0",
        "eslint": "7.15.0",
        "eslint-config-airbnb-typescript": "12.0.0",
        "eslint-config-prettier": "7.0.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "eslint-plugin-prettier": "3.3.0",
