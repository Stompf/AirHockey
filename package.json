{
    "name": "lunne-web",
    "version": "1.0.0",
    "description": "Lunne Web",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start-server\" \"npm run start-client-dev\"",
        "start-server": "nodemon",
        "server-watch": "tsc -p ./src/server/tsconfig.json --watch",
        "build-server-prod": "tsc -p ./src/server/tsconfig.json",
        "start-client-dev": "webpack --config=./webpack.dev.js --mode development --watch",
        "build-client-prod": "webpack --config=./webpack.prod.js --mode production",
        "build-all-prod": "npm run build-client-prod && npm run build-server-prod",
        "start-prod": "NODE_ENV=production node --experimental-worker server-dist/server/index.js",
        "build-docker-prod": "docker build -t stompf/lunne-web  .",
        "push-to-docker": "docker login && docker push stompf/lunne-web:latest",
        "build-and-push": "npm run build-docker-prod && npm run push-to-docker"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Stompf/AirHockey.git"
    },
    "author": "Anton Lundmark",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Stompf/AirHockey/issues"
    },
    "homepage": "https://github.com/Stompf/AirHockey#readme",
    "dependencies": {
        "express": "^4.16.4",
        "helmet": "^3.16.0",
        "matter-js": "^0.14.2",
        "module-alias": "^2.2.0",
        "p2": "^0.7.1",
        "phaser": "^3.16.2",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@types/express": "^4.16.1",
        "@types/helmet": "0.0.43",
        "@types/matter-js": "^0.10.2",
        "@types/module-alias": "^2.0.0",
        "@types/node": "^10.14.5",
        "@types/p2": "^0.7.32",
        "@types/socket.io": "^2.1.2",
        "@types/socket.io-client": "^1.4.32",
        "babel-loader": "^8.0.5",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^4.1.0",
        "copy-webpack-plugin": "^5.0.3",
        "husky": "^1.3.1",
        "nodemon": "^1.18.11",
        "prettier": "^1.17.0",
        "ts-loader": "^5.4.4",
        "ts-node": "^8.1.0",
        "tsconfig-paths": "^3.8.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "tslint": "^5.16.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.4.5",
        "uglifyjs-webpack-plugin": "^2.1.2",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.1",
        "webpack-merge": "^4.2.1"
    },
    "_moduleAliases": {
        "src": "./server-dist"
    }
}
