{
    "name": "lunne-web",
    "version": "1.0.0",
    "description": "Lunne Web",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start-server\" \"npm run start-client-dev\"",
        "start-server": "nodemon",
        "server-watch": "tsc -p ./src/server/tsconfig.json --watch",
        "build-server-prod": "tsc -p ./src/server/tsconfig.json",
        "start-client-dev": "webpack --config=./webpack.dev.js --mode development --watch",
        "build-client-prod": "webpack --config=./webpack.prod.js --mode production",
        "build-all-prod": "npm run build-client-prod && npm run build-server-prod",
        "start-prod": "NODE_ENV=production node server-dist/server/index.js",
        "build-docker-prod": "docker build -t stompf/lunne-web  .",
        "push-to-docker": "docker login && docker push stompf/lunne-web:latest",
        "build-and-push": "npm run build-docker-prod && npm run push-to-docker",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Stompf/AirHockey.git"
    },
    "author": "Anton Lundmark",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Stompf/AirHockey/issues"
    },
    "homepage": "https://github.com/Stompf/AirHockey#readme",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "event-emitter": "^0.3.5",
        "express": "^4.17.1",
        "helmet": "^3.23.3",
        "matter-js": "^0.14.2",
        "module-alias": "^2.2.2",
        "moment": "^2.27.0",
        "morgan": "^1.10.0",
        "p2": "^0.7.1",
        "phaser": "^3.24.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.10.5",
        "@types/event-emitter": "^0.3.3",
        "@types/express": "^4.17.7",
        "@types/helmet": "0.0.47",
        "@types/matter-js": "^0.14.5",
        "@types/module-alias": "^2.0.0",
        "@types/morgan": "^1.9.1",
        "@types/node": "^12.12.52",
        "@types/p2": "^0.7.36",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/react-router": "^5.1.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/socket.io": "^2.1.10",
        "@types/socket.io-client": "^1.4.33",
        "@typescript-eslint/eslint-plugin": "^3.7.0",
        "@typescript-eslint/parser": "^3.7.0",
        "babel-loader": "^8.1.0",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^5.2.0",
        "copy-webpack-plugin": "^6.0.3",
        "eslint": "^7.5.0",
        "eslint-config-airbnb-typescript": "^9.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prefer-arrow": "^1.2.2",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "eslint-plugin-react-hooks": "^4.0.8",
        "husky": "^4.2.5",
        "nodemon": "^2.0.4",
        "prettier": "^2.0.5",
        "ts-loader": "^8.0.1",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.9.7",
        "webpack": "^4.44.0",
        "webpack-cli": "^3.3.12",
        "webpack-merge": "^5.0.9"
    },
    "_moduleAliases": {
        "src": "./server-dist"
    }
}
