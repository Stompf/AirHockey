{
    "name": "lunne-web",
    "version": "1.0.0",
    "description": "Lunne Web",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start-server\" \"npm run start-client-dev\"",
        "start-server": "nodemon",
        "server-watch": "tsc -p ./src/server/tsconfig.json --watch",
        "build-server-prod": "tsc -p ./src/server/tsconfig.json",
        "start-client-dev": "webpack --config=./webpack.dev.js --mode development --watch",
        "build-client-prod": "webpack --config=./webpack.prod.js --mode production",
        "build-all-prod": "npm run build-client-prod && npm run build-server-prod",
        "start-prod": "NODE_ENV=production node server-dist/server/index.js",
        "build-docker-prod": "docker build -t stompf/lunne-web  .",
        "push-to-docker": "docker login && docker push stompf/lunne-web:latest",
        "build-and-push": "npm run build-docker-prod && npm run push-to-docker",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Stompf/AirHockey.git"
    },
    "author": "Anton Lundmark",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Stompf/AirHockey/issues"
    },
    "homepage": "https://github.com/Stompf/AirHockey#readme",
    "dependencies": {
        "@material-ui/core": "^4.9.0",
        "@material-ui/icons": "^4.5.1",
        "event-emitter": "^0.3.5",
        "express": "^4.17.1",
        "helmet": "^3.21.2",
        "matter-js": "^0.14.2",
        "module-alias": "^2.2.2",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "p2": "^0.7.1",
        "phaser": "^3.22.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@types/event-emitter": "^0.3.3",
        "@types/express": "^4.17.2",
        "@types/helmet": "0.0.45",
        "@types/matter-js": "^0.10.9",
        "@types/module-alias": "^2.0.0",
        "@types/morgan": "^1.7.37",
        "@types/node": "^12.12.21",
        "@types/p2": "^0.7.36",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/react-router": "^5.1.4",
        "@types/react-router-dom": "^5.1.3",
        "@types/socket.io": "^2.1.4",
        "@types/socket.io-client": "^1.4.32",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "babel-loader": "^8.0.6",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^5.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-typescript": "^6.3.1",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prefer-arrow": "^1.1.7",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.2",
        "eslint-plugin-react-hooks": "^1.7.0",
        "husky": "^4.2.1",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-merge": "^4.2.2"
    },
    "_moduleAliases": {
        "src": "./server-dist"
    }
}
